    "pm2:restart": "pm2 restart newwaapi",
    "pm2:stop": "pm2 stop newwaapi",
    "pm2:status": "pm2 show newwaapi",
    "pm2:delete": "pm2 delete newwaapi",
    "pm2:logs": "pm2 logs newwaapi --lines 1000"
  },
